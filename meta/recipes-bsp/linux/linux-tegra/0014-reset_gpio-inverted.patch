-		__overlay__ {
-			status = "okay";
-			port-index = <0>;
-			bus-width = <2>;
-			remote-endpoint = <&rbpcv2_dione1280_csi_out0>;
-		};
-	};
-	fragment@4 {
-		target = <&rbpcv2_dione1280_csi_in0>;
