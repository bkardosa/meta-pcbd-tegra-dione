From d9df1de9a4148d7bff0bd533ebd3f2fd3d2d6ddc Mon Sep 17 00:00:00 2001
From: Botond Kardos <bkardos70@gmail.com>
Date: Thu, 10 Feb 2022 13:07:55 +0100
Subject: [PATCH] dione_ir: reset_gpio inverted, unneeded files removed

---
 nvidia/drivers/media/i2c/dione_ir.c           | 12 ++--
 .../platform/t210/batuu/kernel-dts/Makefile   |  2 -
 .../kernel-dts/camera-overlay-dione1280.dts   | 60 -------------------
 .../kernel-dts/camera-overlay-dione640.dts    | 59 ------------------
 .../porg-platforms/tegra210-porg-camera.dtsi  |  3 +-
 .../tegra210-porg-p3448-common.dtsi           |  2 -
 6 files changed, 7 insertions(+), 131 deletions(-)
 delete mode 100644 nvidia/platform/t210/batuu/kernel-dts/camera-overlay-dione1280.dts
 delete mode 100644 nvidia/platform/t210/batuu/kernel-dts/camera-overlay-dione640.dts

diff --git a/nvidia/drivers/media/i2c/dione_ir.c b/nvidia/drivers/media/i2c/dione_ir.c
index 372ffdad1cac..439d8eac62b0 100644
--- a/nvidia/drivers/media/i2c/dione_ir.c
+++ b/nvidia/drivers/media/i2c/dione_ir.c
@@ -313,9 +313,9 @@ static int dioneir_power_on(struct camera_common_data *s_data)
 
 	if (pw->reset_gpio) {
 		if (gpio_cansleep(pw->reset_gpio))
-			gpio_set_value_cansleep(pw->reset_gpio, 0);
+			gpio_set_value_cansleep(pw->reset_gpio, 1);
 		else
-			gpio_set_value(pw->reset_gpio, 0);
+			gpio_set_value(pw->reset_gpio, 1);
 	}
 
 	if (unlikely(!(pw->avdd || pw->iovdd || pw->dvdd)))
@@ -346,9 +346,9 @@ static int dioneir_power_on(struct camera_common_data *s_data)
 skip_power_seqn:
 	if (pw->reset_gpio) {
 		if (gpio_cansleep(pw->reset_gpio))
-			gpio_set_value_cansleep(pw->reset_gpio, 1);
+			gpio_set_value_cansleep(pw->reset_gpio, 0);
 		else
-			gpio_set_value(pw->reset_gpio, 1);
+			gpio_set_value(pw->reset_gpio, 0);
 	}
 
 	usleep_range(23000, 23100);
@@ -392,9 +392,9 @@ static int dioneir_power_off(struct camera_common_data *s_data)
 	} else {
 		if (pw->reset_gpio) {
 			if (gpio_cansleep(pw->reset_gpio))
-				gpio_set_value_cansleep(pw->reset_gpio, 0);
+				gpio_set_value_cansleep(pw->reset_gpio, 1);
 			else
-				gpio_set_value(pw->reset_gpio, 0);
+				gpio_set_value(pw->reset_gpio, 1);
 		}
 
 		usleep_range(10, 10);
